import java.util.ArrayList;

public class dogStore1 extends Dog {

	public dogStore1(String name) {
		super(name);
		// TODO Auto-generated constructor stub
	}
	private static ArrayList <Dog> addDogs = new ArrayList <Dog>();

	public static String getAllDogs(){
		String returnString = "";
		for (int i = 0; i <addDogs.size(); i ++) {
			returnString = ( getDogName());
		}
		return returnString;
	}
	
	public static void addDogs(String name){
		addDogs.add(new Dog(name));
	}
	
}

