
public class Human {
	
	
	private static String Dog;
	private static String name;

	public Human(String name) {
		
			
		if (name.length() >= Constants.MIN_NUMBER_OF_LETTERS){
		Human.name = name;
		
		} if (name.length() < Constants.MIN_NUMBER_OF_LETTERS && name.length()>1 ){
			Human.name = "navn må være mer enn tre bokstaver";
			
		} if (name.length() < 0){
			Human.name = "hunden må ha en eier";
		}
		}
	
	
	
	public static String getName(){
		return name;
	}
	
	
	public static String buyDog(){
		return Dog;
			
	}
	
	//public String getInfo(){	
	
	//}

}
