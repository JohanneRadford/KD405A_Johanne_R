
public class Snake extends Animal {

	private boolean poisonous = true;
	private String info;
	
	
	public Snake(String latinName, boolean poisonous) {
		super(latinName);
		this.poisonous = poisonous;
	}
	
	public boolean isPoisonous(){
		return poisonous;
	}

	@Override
	public String getInfo() {
	if(poisonous = true){
		info = isPoisonous() + " is poisonous" ;
	}else{
	info = 	isPoisonous() + " is not poisonous ";
	
	}
		return info;
	}
	
}

 

