
import java.util.ArrayList;

public class CarGarden {

	private ArrayList<Cat> allCats;
	private String name;
	
	public CatGarden (String name){
		allCats = new ArrayList<Cat>();
		this.name = name;
	}
	
	public void addCat(Cat c){
		if ( allCats.size() <101){
			this.allCats.add(c);
		}
	}
	
	public void killCat(){
		allCats.remove();
	}
	

}
